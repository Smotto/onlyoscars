extends layout

block content
    .head
        <meta name="viewport" content="width=device-width, initial-scale=1">
        .body
            h1#SpecificTitleID #{SpecificTitle}
                a(href='http://localhost:3000/')
                    button.btn.Home HOME
            .position
                .wrapper
                    div#movieDataID(hidden='true') #{filteringFindings}
                    table#movieDataTable(style='width:100%')
                        tr
                            th#year-header Year
                            th#category-header Category
                            th#entity-header Movie Name
                            th#winner-header Winner















    script(type = module src='/javascripts/specificPage.js')

